{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Algol68",
  "patterns": [
    {
      "include": "#comment"
    },
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    }
  ],
  "repository": {
    "comment": {
      "patterns": [
        {
          "name": "comment.multiline.algol68",
          "begin": "#",
          "end": "#"
        }
      ]
    },
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.algol68",
          "match": "\\b(BEGIN|END|TO|DO|OD|INT|REAL|BOOL|CHAR|STRUCT)\\b"
        }
      ]
    },
    "strings": {
      "name": "string.quoted.double.algol68",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.algol68",
          "match": "\\\\."
        }
      ]
    }
  },
  "scopeName": "text.algol.algol68"
}
